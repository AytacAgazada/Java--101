public class code {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);

        System.out.print("N sayinizi giriniz: ");
        int n=input.nextInt();

        double result=0.0;

        for (double i=1;i<=n;i++){
            result+=(1/i); //ve ya 1.0 yazaraq double putput almaq mumkundur
        }
        System.out.print("Harmonik sayiniz: " + result);
}
}
